<?xml version="1.0" encoding="UTF-8"?>
<phpunit>
     <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">./src</directory>
        </whitelist>
    </filter>
    <logging>
        <log type="coverage-php" target="build/cov/coverage.cov"/>
    </logging>
</phpunit>
